    "version": "0.2.0",
    "configurations": 
    [
        {
            "name": "x64-Dist",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}\\build\\Debug\\WindowEngine.exe",
            "symbolSearchPath": "C:\\Symbols;C:\\SymbolDir2",
